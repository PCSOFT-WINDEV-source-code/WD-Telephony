   -
     name : CallStatus
     internal_properties : CAAAAAgAAADiYz012z5enY5O/xgnfQMeGBUqtToHgcgFL1qUiHWgb+aPtelvO1l44TLL+TD09pGEL7EQGsxTVoF5Vlg9PHt+29lBdhbMcaBp4lhTkWyRFsVKCSDT/pHS4IqPyDFEMei1upib8IVOZZpuxt05
     procedure_id : 1760357178817904230
     type_code : 14
     code : |1-
      // This procedure is called by the WLanguage function named "tapiDial" as CallBack
      PROCEDURE CallStatus(nCallStatus is int)
      
      	// Indicate the status of the connection to the user 
      	SWITCH nCallStatus
      		CASE tapiLineBusy : 
      			// Line busy
      			Message("The line is busy")
      	
      		CASE tapiLineConnected : 
      			//  Line picked up
      			Message("Connection established")
      	
      		CASE tapiLineDialing : 
      			// Dialling in progress
      			Message("Dialing in progress")	
      	
      		CASE tapiLineDialTone : 
      			// Receive the dial tone
      			Message("Receive the dial tone...")
      	
      		CASE tapiLineDisconnected : 
      			Message("The correspondent has hung up")
      	
      		CASE tapiLineProceeding : 
      			Message("Searching for correspondent")
      	
      		CASE tapiLineRingBack : 
      			Message("Ringing")
      	
      		OTHER CASE
      			
      	END
     type : 458752
   -
     name : Dial
     procedure_id : 1760357178817969766
     type_code : 14
     code : |1-
      PROCEDURE Dial(sPhoneNumber is string)
      	
      	// Dial the number
      	IF tapiDial(sPhoneNumber, CallStatus, 15) THEN
      		// Communication established	
      		Message("The communication is established")
      		Info("The communication is established, please pick up")		
      		RETURN
      	ELSE
      		// Error during the composition of the number
      		IF NoSpace(ErrorInfo()) <> "" THEN
      			Error(ErrorInfo())
      			Close
      		END
      		
      		// Check whether the line is busy
      		IF tapiCallIsBusy() THEN
      			Info("The line is busy")
      			Close
      		END
      		
      		// Check whether the correspondent responded
      		IF tapiNoAnswer() THEN
      			Info("the correspondent did not respond")
      			Close
      		END	
      	END
     type : 458752
